<div class="container"
  <h2>Edit Project</h2>
  <form class="form-group" action="/edit/<%= @project.id %>" method="post">
    <label for="">Project Title</label>
    <input type="text" name="title" value="<%= @project.title %>">
    <button type="submit">Update Project</button>
    <button type="button" action="/" method="get">Cancel</button>
  </form>
</div>
